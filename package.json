{
  "name": "tsuite",
  "version": "0.5.5",
  "description": "A collection of useful utility functions, All fully typed and documented",
  "main": "dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./create-node": {
      "import": "./dist/create-node.mjs",
      "require": "./dist/create-node.js"
    },
    "./create-request": {
      "import": "./dist/create-request.mjs",
      "require": "./dist/create-request.js"
    },
    "./create-state": {
      "import": "./dist/create-state.mjs",
      "require": "./dist/create-state.js"
    },
    "./effetch": {
      "import": "./dist/effetch.mjs",
      "require": "./dist/effetch.js"
    },
    "./hug-text": {
      "import": "./dist/hug-text.mjs",
      "require": "./dist/hug-text.js"
    },
    "./load-image-with-fallback": {
      "import": "./dist/load-image-with-fallback.mjs",
      "require": "./dist/load-image-with-fallback.js"
    },
    "./map-elements-by-id": {
      "import": "./dist/map-elements-by-id.mjs",
      "require": "./dist/map-elements-by-id.js"
    },
    "./toast": {
      "import": "./dist/toast.mjs",
      "require": "./dist/toast.js"
    },
    "./try-catch": {
      "import": "./dist/try-catch.mjs",
      "require": "./dist/try-catch.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --clean",
    "build:docs": "typedoc --plugin typedoc-plugin-rename-defaults",
    "fmt": "bun x prettier --write ."
  },
  "keywords": [
    "typescript",
    "utility",
    "functions",
    "try-catch",
    "error-handling",
    "dom",
    "elements",
    "state",
    "request",
    "fetch",
    "toast",
    "image",
    "fallback"
  ],
  "author": "tijnjh",
  "authors": [
    "Tijn/Jonas <tijn@tijn.dev>"
  ],
  "homepage": "https://tijnjh.github.io/tsuite",
  "repository": "tijnjh/tsuite",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "csstype": "^3.1.3",
    "tslib": "^2.8.1",
    "tsup": "^8.4.0",
    "typedoc": "^0.28.3",
    "typedoc-plugin-rename-defaults": "^0.7.3",
    "typescript": "^5.8.3"
  }
}
