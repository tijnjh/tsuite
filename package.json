{
  "name": "tsuite",
  "version": "0.6.3",
  "private": false,
  "description": "A collection of useful utility functions, All fully typed and documented",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "tsdown --watch",
    "build": "tsdown --clean",
    "build:docs": "typedoc",
    "fmt": "bun x prettier --write ."
  },
  "keywords": [
    "typescript",
    "utility",
    "functions",
    "try-catch",
    "error-handling",
    "dom",
    "elements",
    "state",
    "request",
    "fetch",
    "toast",
    "image",
    "fallback"
  ],
  "author": "tijnjh",
  "homepage": "https://tijnjh.github.io/tsuite",
  "repository": "tijnjh/tsuite",
  "license": "AGPL-3.0-only",
  "devDependencies": {
    "@types/node": "^22.14.1",
    "csstype": "^3.1.3",
    "tsdown": "^0.12.6",
    "tslib": "^2.8.1",
    "typedoc": "^0.28.3",
    "typescript": "^5.8.3"
  },
  "exports": {
    ".": "./dist/index.mjs",
    "./create-node": "./dist/create-node.mjs",
    "./create-request": "./dist/create-request.mjs",
    "./create-state": "./dist/create-state.mjs",
    "./effetch": "./dist/effetch.mjs",
    "./hug-text": "./dist/hug-text.mjs",
    "./load-image-with-fallback": "./dist/load-image-with-fallback.mjs",
    "./map-elements-by-id": "./dist/map-elements-by-id.mjs",
    "./package.json": "./package.json"
  },
  "dependencies": {
    "easy-try-catch": "^0.2.0"
  }
}
